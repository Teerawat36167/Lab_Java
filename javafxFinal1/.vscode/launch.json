{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch App",
      "request": "launch",
      "mainClass": "App",
      "vmArgs": "--module-path \"lib/javafx/lib\" --add-modules javafx.controls,javafx.fxml,javafx.media"
    }
  ]
}
